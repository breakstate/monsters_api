#!/usr/bin/env node
/***************************************/
/* ./bin/www                           */
/*                                     */
/* Initializes web server              */
/* port: 3000                          */
/* Used in start and dev npm script    */
/***************************************/

const app = require('../app');

const port = 3000;
app.listen(port, () => console.log(`listening on port ${port}`));
